{
  "name": "kizunacoin",
  "description": "Smart payments made simple",
  "author": "Kizunacoin",
  "version": "2.6.0",
  "keywords": [
    "wallet",
    "kizunacoin",
    "decentralized",
    "multisignature",
    "bitcoin"
  ],
  "main": "public/index.html",
  "window": {
    "title": "Kizunacoin wallet",
    "icon": "./public/img/icons/logo-circle-256.png",
    "show": true,
    "visible": true,
    "resizable": false,
    "frame": true,
    "width": 400,
    "height": 600,
    "position": "center",
    "fullscreen": false
  },
  "webkit": {
    "page-cache": false,
    "java": false,
    "plugin": false
  },
  "chromium-args": "--proxy-server=http://127.1.2.7 --password-store=basic --disable-password-generation",
  "homepage": "https://github.com/KIZUNACOIN/kizunacoin",
  "license": "MIT",
  "repository": {
    "url": "git://github.com/KIZUNACOIN/kizunacoin.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/KIZUNACOIN/kizunacoin/issues"
  },
  "browser": {
    "request": "browser-request",
    "secp256k1": "secp256k1/js"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bip38": "^1.4.0",
    "bitcore-lib": "^0.13.14",
    "bitcore-mnemonic": "~1.0.0",
    "core": "git+https://github.com/KIZUNACOIN/core.git",
    "grunt": "^1.0.1",
    "grunt-angular-gettext": "^2.2.3",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-compress": "^1.3.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^1.0.0",
    "jszip": "^3.1.3",
    "lodash": "^4.6.1",
    "moment": "^2.18.1",
    "preconditions": "^1.0.8",
    "shelljs": "^0.3.0",
    "sjcl": "^1.0.2",
    "unzip": "git+https://github.com/xJeneKx/unzip.git",
    "zip": "git+https://github.com/xJeneKx/zip.git"
  },
  "optionalDependencies": {
    "innosetup-compiler": "^5.5.9"
  },
  "scripts": {
    "test": "./node_modules/.bin/grunt test-coveralls"
  },
  "devDependencies": {
    "angular-mocks": "^1.2.0",
    "grunt-karma": "2",
    "grunt-karma-coveralls": "2",
    "grunt-nw-builder": "^2.0.0",
    "jasmine-core": "^3.1.0",
    "karma": "2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.16"
  }
